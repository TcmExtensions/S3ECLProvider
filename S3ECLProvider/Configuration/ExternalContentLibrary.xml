<Configuration xmlns="http://www.sdltridion.com/ExternalContentLibrary/Configuration">
  <!-- Available logging levels: Debug, Info, Warning, Error -->
  <l:Logging Level="Warning" xmlns:l="http://www.sdltridion.com/Infrastructure/LoggingConfiguration">
    <!-- Additional supported attributes: Language="rfc1766 code" Locale="rfc1766 code". An example of an rfc1766 code is "de-DE" for German. /-->
    <!-- <l:Folder>Full path to logging folder, default is %TRIDION%/bin/log</l:Folder>-->
  </l:Logging>
  <CoreServiceUrl>net.tcp://localhost:2660/CoreService/2011/netTcp</CoreServiceUrl>
  <MountPoints>
    <MountPoint type="S3ECLProvider" version="*" id="s3" rootItemName="AWS S3 Bucket">
      <StubFolders>
        <StubFolder id="tcm:2-84-2" />        
      </StubFolders>
      <PrivilegedUserName>WIN-PKT1MENDBR7\Administrator</PrivilegedUserName>      
      <EnableUpload administrators="true"/>
      <FullBucketUrl xmlns="http://www.sdltridion.com/S3EclProvider/Configuration">full bucket Url with / at the end</FullBucketUrl>
      <S3BucketName xmlns="http://www.sdltridion.com/S3EclProvider/Configuration">bucket name, no /</S3BucketName>
      <S3AccessId xmlns="http://www.sdltridion.com/S3EclProvider/Configuration">access id</S3AccessId>
      <S3SecretKey xmlns="http://www.sdltridion.com/S3EclProvider/Configuration">secret key</S3SecretKey>
      <ECLCoreServiceUrl xmlns="http://www.sdltridion.com/S3EclProvider/Configuration">netTcp endpoint</ECLCoreServiceUrl>
    </MountPoint>
  </MountPoints>
</Configuration>

